{
    "name": "embarknow/value-objects",
    "description": "A collection of value object classes.",
    "license": "MIT",
    "authors": [
        {
            "name": "John Porter",
            "email": "john@embarknow.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url": "git://github.com/embarknow/value-objects"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ramsey/uuid": "~3.1",
        "marc-mabe/php-enum": "~1.0"
